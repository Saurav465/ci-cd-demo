<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <display-name>Sampleproject</display-name>
    <servlet>
    	<description>
    	</description>
    	<display-name>XMLtoJSON</display-name>
    	<servlet-name>XMLtoJSON</servlet-name>
    	<servlet-class>com.test.XMLtoJSON</servlet-class>
    </servlet>
    <servlet>
    	<description>
    	</description>
    	<display-name>JSONtoXML</display-name>
    	<servlet-name>JSONtoXML</servlet-name>
    	<servlet-class>com.test.JSONtoXML</servlet-class>
    </servlet>
    <servlet>
    	<description>
    	checking the tps</description>
    	<display-name>TestTPS</display-name>
    	<servlet-name>TestTPS</servlet-name>
    	<servlet-class>com.tps.demo.TestTPS</servlet-class>
    </servlet>
    <servlet>
    	<description>
    	</description>
    	<display-name>TPS2</display-name>
    	<servlet-name>TPS2</servlet-name>
    	<servlet-class>com.tps.demo.TPS2</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>XMLtoJSON</servlet-name>
    	<url-pattern>/XMLtoJSON</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>JSONtoXML</servlet-name>
    	<url-pattern>/JSONtoXML</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>TestTPS</servlet-name>
    	<url-pattern>/TestTPS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>TPS2</servlet-name>
    	<url-pattern>/TPS2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        </welcome-file-list>
</web-app>
